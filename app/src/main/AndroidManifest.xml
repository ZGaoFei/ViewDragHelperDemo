<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zgf.viewdraghelperdemo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".dragview.DragViewActivity"></activity>
        <activity android:name=".dragmultiview.DragMultiTestActivity" />
        <activity
            android:name=".DragTestActivity"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" />
        <activity android:name=".swipedelete.SwipeDeleteActivity" />
        <activity
            android:name=".swipeback.SwipeBackThreeActivity"
            android:theme="@style/MyAppTheme" />
        <activity
            android:name=".swipeback.SwipeBackTwoActivity"
            android:theme="@style/MyAppTheme" />
        <activity
            android:name=".swipeback.SwipeBackFirstActivity"
            android:theme="@style/MyAppTheme" />
        <activity android:name=".TestOneActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>